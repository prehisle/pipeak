  "knowledgeContent": {
    "lesson1": {
      "mathEnvironment": {
        "title": "LaTeX Math Environment",
        "content": "LaTeX mathematical formulas need to be written in specific environments:\n\n‚Ä¢ **Inline Formula**: Use `$...$` to wrap, such as `$x^2$` ‚Üí $x^2$\n‚Ä¢ **Display Formula**: Use `$$...$$` to wrap, such as `$$E = mc^2$$` ‚Üí $$E = mc^2$$"
      },
      "superscriptSubscript": {
        "title": "Superscripts and Subscripts",
        "content": "‚Ä¢ Use `^` for superscripts: `$x^2$` ‚Üí $x^2$\n‚Ä¢ Use `_` for subscripts: `$x_1$` ‚Üí $x_1$\n‚Ä¢ Use both together: `$x_1^2$` ‚Üí $x_1^2$"
      }
    },
    "lesson2": {
      "fractions": {
        "title": "Fractions",
        "content": "Use `\\frac{numerator}{denominator}` command:\n\n‚Ä¢ `$\\frac{1}{2}$` ‚Üí $\\frac{1}{2}$\n‚Ä¢ `$\\frac{a+b}{c-d}$` ‚Üí $\\frac{a+b}{c-d}$\n‚Ä¢ `$\\frac{x^2}{y^3}$` ‚Üí $\\frac{x^2}{y^3}$"
      },
      "radicals": {
        "title": "Radicals",
        "content": "Use `\\sqrt{}` command:\n\n‚Ä¢ `$\\sqrt{x}$` ‚Üí $\\sqrt{x}$ (square root)\n‚Ä¢ `$\\sqrt[3]{x}$` ‚Üí $\\sqrt[3]{x}$ (cube root)\n‚Ä¢ `$\\sqrt{x^2 + y^2}$` ‚Üí $\\sqrt{x^2 + y^2}$"
      }
    },
    "lesson3": {
      "greekLetters": {
        "title": "Greek Letters",
        "content": "‚Ä¢ `$\\alpha$` ‚Üí $\\alpha$ (alpha)\n‚Ä¢ `$\\beta$` ‚Üí $\\beta$ (beta)\n‚Ä¢ `$\\gamma$` ‚Üí $\\gamma$ (gamma)\n‚Ä¢ `$\\delta$` ‚Üí $\\delta$ (delta)\n‚Ä¢ `$\\pi$` ‚Üí $\\pi$ (pi)\n‚Ä¢ `$\\theta$` ‚Üí $\\theta$ (theta)\n‚Ä¢ `$\\lambda$` ‚Üí $\\lambda$ (lambda)\n‚Ä¢ `$\\mu$` ‚Üí $\\mu$ (mu)"
      },
      "capitalGreekLetters": {
        "title": "Capital Greek Letters",
        "content": "‚Ä¢ `$\\Gamma$` ‚Üí $\\Gamma$ (Gamma)\n‚Ä¢ `$\\Delta$` ‚Üí $\\Delta$ (Delta)\n‚Ä¢ `$\\Theta$` ‚Üí $\\Theta$ (Theta)\n‚Ä¢ `$\\Lambda$` ‚Üí $\\Lambda$ (Lambda)\n‚Ä¢ `$\\Pi$` ‚Üí $\\Pi$ (Pi)\n‚Ä¢ `$\\Sigma$` ‚Üí $\\Sigma$ (Sigma)\n‚Ä¢ `$\\Omega$` ‚Üí $\\Omega$ (Omega)"
      },
      "commonSymbols": {
        "title": "Common Mathematical Symbols",
        "content": "‚Ä¢ `$\\infty$` ‚Üí $\\infty$ (infinity)\n‚Ä¢ `$\\pm$` ‚Üí $\\pm$ (plus-minus)\n‚Ä¢ `$\\times$` ‚Üí $\\times$ (times)\n‚Ä¢ `$\\div$` ‚Üí $\\div$ (division)\n‚Ä¢ `$\\neq$` ‚Üí $\\neq$ (not equal)\n‚Ä¢ `$\\leq$` ‚Üí $\\leq$ (less than or equal)\n‚Ä¢ `$\\geq$` ‚Üí $\\geq$ (greater than or equal)\n‚Ä¢ `$\\approx$` ‚Üí $\\approx$ (approximately equal)"
      }
    }
  }
